<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.serviceapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ServiceApp">
        <activity android:name=".Register"></activity>
        <activity android:name=".Login" />
        <activity android:name=".Plumber" />
        <activity android:name=".Mechanic" />
        <activity android:name=".Painter" />
        <activity android:name=".Carpenter" />
        <activity android:name=".Constructor" />
        <activity android:name=".Electrician" />
        <activity android:name=".Lobby" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>